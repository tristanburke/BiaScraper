{
  "_removeNodes": [
    "//script",
    "//style"
  ],
  "title": "//div[contains(@class, 'story-body')]//h1",
  "dateTime":
  {
    "_xpath": "//div[contains(@class, 'story-body')]//div[contains(@class, 'date')]",
    "_mapTransformations":
    [
      {
        "_type": "ParseDateTransformation",
        "_format": "YYYY-MM-DD"
      }
    ]
  },
  "body": "//div[@property='articleBody']"
}