{
  "_removeNodes": [
    "//script",
    "//style"
  ],
  "products": 
  {
    "_xpath": "//div[@id='productLists']//div[starts-with(@id, 'item_')]",
    "title": ".//div[contains(@class, 'productTitle')]",
    "description": ".//div[contains(@class, 'productDesp')]",
    "price": 
    {
      "_xpath": ".//div[contains(@class, 'price')]/text()[1]",
      "_mapTransformations": [
        "TrimTransformation"
      ]
    }
  }
}